//L006

public class Main
{
	public static void main(String[] args) {
		 int [] arr = {10, 20,30,40};
		 for(int si = 0; si<arr.length; si++){
		     for(int ei = si; ei<arr.length; ei++){
		         for(int k = si; k<=ei; k++){
		             System.out.print(arr[k] + " ");
		         }
		         System.out.println();
		     }
		 }
	}
}

