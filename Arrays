L005
// sum of two array
import java.util.*;
public class Main
{
    
	public static void main(String[] args) {
	    int a1[] = {9, 2,7,8,9};
	    int a2[] = {4,7,1,3,6,9,3};
	    int l1 = a1.length;
	    int l2 = a2.length;
	    int s = 0;
	    if(l1>l2){
	        s = l1 +1;
	    } else {
	        s = l2 +1;
	    }
	    
        int ans[] = new int[s];
        int i = a1.length - 1;
        int j = a2.length - 1;
        int k = ans.length - 1;
        int carry = 0;
        while(k>=0){
            int sum = 0;
            if(i>=0){
                sum = sum + a1[i];
            }
            
            if(j>=0){
                sum = sum + a2[j];
            }
            sum = sum +carry;
            ans[k] = sum %10;
            carry = sum/10;
            
            i--;
            j--;
            k--;
        }
	    
	     for(int p = 0; p<ans.length; p++){
	         if(p==0 && ans[p] == 0 ) {
	              continue;
	         } 
	             System.out.print(ans[p] + " ");
	             
	     }
	     
	}
}

